{
  "name": "memoryofthepst",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "cz",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "axios": "^1.7.8",
    "pnpm": "^10.22.0",
    "vue": "^3.5.24",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.1",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue-tsc": "^3.1.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,vue}": [
      "prettier --write"
    ],
    "src/**/*.{css,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
